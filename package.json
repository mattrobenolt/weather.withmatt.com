{
  "name": "weather.withmatt.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "preact build --no-sw --no-esm --no-prerender",
    "dev": "preact watch -H 127.0.0.1 -p 3000 --no-sw --no-esm --no-prerender --template=src/template.html",
    "lint": "eslint src",
    "format": "prettier --write src",
    "deploy": "vercel deploy --prod",
    "clean": "rm -rf build"
  },
  "eslintConfig": {
    "extends": "preact"
  },
  "volta": {
    "node": "16.17.1"
  },
  "browserslist": ["last 1 version"],
  "devDependencies": {
    "eslint": "^8.24.0",
    "preact-cli": "^3.4.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vercel": "^28.4.5"
  },
  "dependencies": {
    "@planetscale/database": "^1.3.0",
    "d3": "^7.6.1",
    "preact": "^10.11.0"
  }
}
